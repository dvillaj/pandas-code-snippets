      "code": [
         "import duckdb",
         "import pandas as pd",
         "",
         "dataframe = pd.read_excel(\"https://bit.ly/Tables-xlsx\") ",
         "",
         "conn = duckdb.connect('data.duckdb')",
         "cursor = conn.cursor()",
         "",
         "cursor.execute(\"\"\"",
         "CREATE TABLE my_table (",
         "    Country      varchar not null,",
         "    Year         int not null,",
         "    Cases        int,",
         "    Population   int",
         ")",
         "\"\"\")",
         "",
         "conn.execute(\"INSERT INTO my_table SELECT * FROM dataframe\")",
         "conn.close()"
      ],
