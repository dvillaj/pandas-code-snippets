      "code": [
         "import pandas as pd",
         "",
         "text_file = open(\"data.csv\", \"w\")",
         "text_file.write(",
         "\"\"\"",
         "Film",
         "Toy Story (1995)",
         "GoldenEye (1996)",
         "Four Rooms (1995)",
         "Get Shorty (1995)",
         "Copycat (1998)",
         "\"\"\")",
         "text_file.close()",
         "",
         "dataframe = pd.read_csv(\"data.csv\")",
         "",
         "regex = r'([\\w ]*) \\((\\d{4})\\)'",
         "",
         "dataframe.assign(",
         "     Title = lambda dataset: dataset.Film.str.extract(regex)[0],",
         "     Year = lambda dataset: dataset.Film.str.extract(regex)[1]",
         "   )"
      ],
      "language": "Python"
   },
