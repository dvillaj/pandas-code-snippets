      "display_name": "Filtering NumPy Arrays - Boolean Index (and, or, not)",
      "code": [
         "import numpy as np",
         "",
         "array = np.array([\"Julio\", \"Jose\", \"Alberto\", \"Julio\", \"Nuria\"])",
         "",
         "print(\"Julio or Nuria\")",
         "print(array[(array == \"Julio\") | (array == \"Nuria\")])",
         "",
         "print(\"Julio and Nuria\")",
         "print(array[(array == \"Julio\") & (array == \"Nuria\")])",
         "",
         "print(\"Julio or not Nuria\")",
         "array[(array == \"Julio\") | ~(array == \"Nuria\")]"
      ],
      "language": "Python"
   },
   "schema_name": "code-snippet"
}