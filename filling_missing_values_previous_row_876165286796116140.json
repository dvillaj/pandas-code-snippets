      "code": [
         "import pandas as pd",
         "",
         "text_file = open(\"data.csv\", \"w\")",
         "text_file.write(",
         "\"\"\"",
         "City,Name,Sex,Age",
         "Austin, Sharon,F,17",
         "New York,Kevin,M,34",
         ",John,M,44",
         ",Barbara,F,21",
         "\"\"\")",
         "text_file.close()",
         "",
         "dataframe = pd.read_csv(\"data.csv\")",
         "display(dataframe)",
         "",
         "# Fill forward null values (From a Previos Row)",
         "dataframe.assign(",
         "    City = lambda dataset : dataset.City.fillna(method = 'ffill')",
         ")"
      ],
