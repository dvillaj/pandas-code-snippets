      "code": [
         "import pandas as pd",
         "",
         "dataframe = pd.DataFrame({'Film': ['Toy Story (1995)',",
         "  'GoldenEye (1996)',",
         "  'Four Rooms (1995)',",
         "  'Get Shorty (1995)',",
         "  'Copycat (1998)']})",
         "",
         "regex = r'([\\w ]*) \\((\\d{4})\\)'",
         "",
         "display(dataframe)",
         "",
         "dataframe.assign(",
         "     Title = lambda dataset: dataset.Film.str.extract(regex)[0],",
         "     Year = lambda dataset: dataset.Film.str.extract(regex)[1]",
         "   )"
      ],
      "language": "Python"
   },
