      "code": [
         "import pandas as pd",
         "import numpy as np",
         "",
         "text_file = open(\"data.csv\", \"w\")",
         "text_file.write(",
         "\"\"\"",
         "Name,Sex,City,Age",
         "Sharon,F,Austin,30",
         "Eva,F,Texas,23",
         "Kevin,M,New York,34",
         "Michael,M,New York,44",
         "Barbara,-,New York,21",
         "\"\"\")",
         "text_file.close()",
         "",
         "dataframe = pd.read_csv(\"data.csv\")",
         "",
         "dataframe.info()",
         "dataframe.Sex.value_counts()",
         "",
         "dataframe.assign(",
         "    Sex = lambda dataset: dataset.Sex.replace({'-' : np.nan})",
         ")"
      ],
