      "code": [
         "import pandas as pd",
         "",
         "text_file = open(\"data.csv\", \"w\")",
         "text_file.write(",
         "\"\"\"",
         "Name,Sex,City,Age",
         "Sharon,F,Austin",
         "Eva,F,Texax, 23",
         "Kevin,M,New York,34",
         "Joe,M,Boston,14",
         ",,New York,44",
         "Barbara,,,21",
         "\"\"\")",
         "text_file.close()",
         "",
         "dataframe = pd.read_csv(\"data.csv\")",
         "",
         "display(dataframe)",
         "",
         "# A group of Series",
         "missing_values = {",
         "    'Name' : 'John', ",
         "    'Age' : 17",
         "}",
         "",
         "dataframe.fillna(missing_values)",
         "",
         "# A Serie",
         "dataframe.assign(",
         "    Name = lambda dataset : dataset.Name.fillna('John')",
         ")"
      ],
      "language": "Python"
   },
