         "",
         "# To Show the number of row duplicates",
         "dataframe.duplicated(subset = ['JobNumber']).sum()",
         "",
         "# To Show a percentage of row duplicates",
         "dataframe.duplicated(subset = ['JobNumber']).mean() * 100",
         "",
         "# To show the actual duplicates",
