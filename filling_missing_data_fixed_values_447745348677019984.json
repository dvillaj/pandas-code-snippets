         " 'City': ['Austin', 'Texas', 'New York', 'Boston', 'New York', np.nan],",
         " 'Age': [np.nan, 23.0, 34.0, 14.0, 44.0, 21.0]})",
         "",
         "display(dataframe)",
         "",
         "# A group of Columns",
         "display(dataframe.fillna({'Name' : 'John', 'Age' : 17}))",
         "",
         "# A particular Column",
         "dataframe.assign(",
         "    Name = lambda dataset : dataset.Name.fillna('John')",
         ")"
