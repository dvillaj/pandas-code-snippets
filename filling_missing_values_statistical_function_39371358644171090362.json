      "code": [
         "import pandas as pd",
         "",
         "text_file = open(\"data.csv\", \"w\")",
         "text_file.write(",
         "\"\"\"",
         "Name,Sex,City,Age",
         "Sharon,F,Austin",
         "Eva,F,Texax, 23",
         "Kevin,M,New York,34",
         "Joe,M,Boston,14",
         ",,New York,44",
         "Barbara,,,21",
         "\"\"\")",
         "text_file.close()",
         "",
         "dataframe = pd.read_csv(\"data.csv\")",
         "display(dataframe)",
         "",
         "dataframe.assign(",
         "    Age = lambda dataset : dataset.Age.fillna(dataset.Age.mean())",
         ")"
      ],
